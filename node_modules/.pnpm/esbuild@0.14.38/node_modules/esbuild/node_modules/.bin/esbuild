#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/angularning/myProject/o/vue3-class/node_modules/.pnpm/esbuild@0.14.38/node_modules/esbuild/bin/node_modules:/Users/angularning/myProject/o/vue3-class/node_modules/.pnpm/esbuild@0.14.38/node_modules/esbuild/node_modules:/Users/angularning/myProject/o/vue3-class/node_modules/.pnpm/esbuild@0.14.38/node_modules:/Users/angularning/myProject/o/vue3-class/node_modules/.pnpm/node_modules:/Users/angularning/myProject/o/vue3-class/node_modules:/Users/angularning/myProject/o/node_modules:/Users/angularning/myProject/node_modules:/Users/angularning/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/angularning/myProject/o/vue3-class/node_modules/.pnpm/esbuild@0.14.38/node_modules/esbuild/bin/node_modules:/Users/angularning/myProject/o/vue3-class/node_modules/.pnpm/esbuild@0.14.38/node_modules/esbuild/node_modules:/Users/angularning/myProject/o/vue3-class/node_modules/.pnpm/esbuild@0.14.38/node_modules:/Users/angularning/myProject/o/vue3-class/node_modules/.pnpm/node_modules:/Users/angularning/myProject/o/vue3-class/node_modules:/Users/angularning/myProject/o/node_modules:/Users/angularning/myProject/node_modules:/Users/angularning/node_modules:/Users/node_modules:/node_modules"
fi
"$basedir/../../bin/esbuild"   "$@"
exit $?
